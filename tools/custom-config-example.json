{
	"project": {
		"name": "SpinalJS",
		"mainSpecPath": "./composer/ui-spec.js"
	},
	"require": {
		"baseURL": "/",
		"paths": {
			"specs": "specs"
		},
		"bundles": {
			"libs": ["libs"],
			"spinal-core": ["spinal-core"],
			"spinal-aop": ["spinal-aop"],
			"spinal-ioc": ["spinal-ioc"],
			"spinal-mvc": ["spinal-mvc"],
			"spinal-ui": ["spinal-ui"],
			"spinal-util": ["spinal-util"]
		},
		"mainConfigFile": "src/main.js",
		"out": "target"
	},
	"options" : {
		"minify": true,
		"banner": "/** MyProject Library Custom Banner **/",
		"license": "path/to/license"
	}
}
